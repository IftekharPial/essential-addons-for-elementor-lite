{
	"name": "essential-addons-for-elementor",
	"version": "1.0.0",
	"description": "=== Essential Addons for Elementor ===\r Contributors: wpdevteam, Codetic, re_enter_rupok, Asif2BD, priyomukul, mahfuz01, nazsabuz\r Tags: elementor, elements, addons, elementor addon, elementor widget, page builder, builder, visual editor, wordpress page builder, elementor form\r Requires at least: 4.0\r Tested up to: 5.2\r Requires PHP: 5.4\r Stable tag: 3.3.0\r License: GPLv3\r License URI: https://opensource.org/licenses/GPL-3.0",
	"scripts": {
		"dev": "webpack --mode production --watch",
		"build": "webpack --mode production && webpack --mode development",
		"lint-php": "wp-env run cli \"sh -c 'cd /var/www/html/wp-content/plugins/essential-addons-for-elementor-lite && php vendor/bin/phpcs'\"",
		"lint-php:fix": "wp-env run cli \"sh -c 'cd /var/www/html/wp-content/plugins/essential-addons-for-elementor-lite && php vendor/bin/phpcbf --report-summary --report-source'\"",
		"reformat-files": "./node_modules/.bin/prettier --ignore-path .eslintignore --write \"**/*.{js,jsx,json,ts,tsx}\"",
		"pretest-unit-php": "wp-env start",
		"test-unit-php": "wp-env run phpunit 'phpunit -c /var/www/html/wp-content/plugins/essential-addons-for-elementor-lite/phpunit.xml.dist --verbose'",
		"wp-env": "wp-env",
		"wp-env:start": "wp-env start",
		"wp-env:setup": "npm run wp-env:composer-install && npm run wp-env:activate-plugin && yarn wp-env run cli wp option update blogname 'EA' && npm run wp-env:load-fixtures",
		"wp-env:composer-install": "wp-env run composer install",
		"wp-env:activate-plugin": "wp-env run cli wp plugin activate essential-addons-for-elementor-lite && wp-env run tests-cli wp plugin activate essential-addons-for-elementor-lite",
		"wp-env:load-fixtures": "wp-env run cli \"sh -c 'cd /var/www/html/wp-content/plugins/essential-addons-for-elementor-lite && php bin/import-fixtures.php --reset'\"",
		"wp-env:stop": "wp-env stop",
		"wp-env:destroy": "wp-env destroy",
		"wp-env:logs": "wp-env logs"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rupok/essential-addons-for-elementor-lite.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/rupok/essential-addons-for-elementor-lite/issues"
	},
	"homepage": "https://github.com/rupok/essential-addons-for-elementor-lite#readme",
	"devDependencies": {
		"@babel/cli": "^7.7.7",
		"@babel/core": "^7.8.6",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/preset-env": "^7.8.6",
		"@wordpress/env": "^4.9.0",
		"autoprefixer": "^9.7.6",
		"babel-loader": "^8.0.6",
		"css-loader": "^3.5.2",
		"glob": "^7.1.6",
		"mini-css-extract-plugin": "^0.9.0",
		"postcss-import": "^12.0.1",
		"postcss-loader": "^3.0.0",
		"sass": "^1.26.3",
		"sass-loader": "^8.0.2",
		"webpack": "^4.42.1",
		"webpack-cli": "^3.3.10",
		"wp-pot": "^1.9.3"
	},
	"dependencies": {
		"@wordpress/hooks": "^2.6.0"
	}
}
